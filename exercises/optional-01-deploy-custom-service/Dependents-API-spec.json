{
	"openapi": "3.0.0",
	"info": {
		"title": "Employee Dependants",
		"version": "1.0.0",
		"description": "\r\n\r\nThis service enables you to retrieve the dependants of an employee."
	},
    "host": "api.domain.test",
    "components": {
        "securitySchemes": {
            "APIKey": {
                "type": "apiKey",
                "name": "api_key",
                "in": "header"
            },
            "OAuth2": {
                "type": "oauth2",
                "flows": {
                    "password": {
                        "tokenUrl": "https://domain.test/api/oauth/token",
                        "scopes": {
                            "read": "example read scope",
                            "write": "example write scope"
                        }
                    }
                }
            }
        }
    }
}